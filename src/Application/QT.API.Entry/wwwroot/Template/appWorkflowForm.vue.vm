<template>
	<view class="qt-wrap qt-wrap-form">
		<u-form :model="@(@Model.FormModel)" :rules="@(@Model.FormRules)" ref="@(@Model.FormModel)" :errorType="['toast']" label-position="@(@Model.LabelPosition)" label-width="@(@Model.LabelWidth)" label-align="left" class="qt-form">
			@{GenCodeAppFormControls();}
		</u-form>
	</view>
</template>
<script>
    import {getDictionaryDataSelector,getDataInterfaceRes} from '@@/api/common'
	import comMixin from '../mixin'
    import request from '@@/utils/request'
    export default {
		mixins: [comMixin],
		name:"@(@Model.EnCode)",
        data() {
            return {
                btnLoading: false,
@foreach(var item in @Model.OptionsList)
{
@if(item.qtKey == "tab")
{
				@:@(@item.LowerName)Current:@(@item.Content),
}
}
                @(@Model.FormModel): {
                    @(@Model.PrimaryKey):'',
					flowId:'',
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "checkbox":
case "cascader":
case "uploadImg":
case "uploadFz":
case "address":
case "comSelect":
@if(item.DefaultValue == null || item.DefaultValue.ToString() == "")
{
					@:@(@item.LowerName):[],
}else{
					@:@(@item.LowerName):@(@item.DefaultValue),
}
break;
case "select":
case "userSelect":
case "treeSelect":
case "depSelect":
case "posSelect":
@if(item.Multiple)
{
@if(item.DefaultValue == null || item.DefaultValue.ToString() == "")
{
					@:@(@item.LowerName):[],
}else{
					@:@(@item.LowerName):@(@item.DefaultValue),
}
}else{
@if(item.DefaultValue == null || @item.DefaultValue.ToString() == "")
{
					@:@(@item.LowerName):undefined,
}else{
					@:@(@item.LowerName):"@(@item.DefaultValue)",
}
}
break;
case "numInput":
case "date":
case "rate":
case "slider":
if(@item.DefaultValue == null || @item.DefaultValue.ToString() == "")
{
					@:@(@item.LowerName):undefined,
}else{
					@:@(@item.LowerName):@(@item.DefaultValue),
}
break;
case "switch":
					@:@(@item.LowerName):@(@item.DefaultValue ? "1" : "0"),
break;
case "table":
					@:@(@item.OriginalName):[],
break;
default:
if(@item.DefaultValue == null || @item.DefaultValue.ToString() == "")
{
					@:@(@item.LowerName):undefined,
}else{
					@:@(@item.LowerName):"@(@item.DefaultValue)",
}
break;
}
}
                },
                rules: {
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "table":
break;
default:
@if(item.Required || (item.RegList!=null && item.RegList.Count > 0))
{
					@:@(@item.LowerName):[
@if(item.Required)
{
						@:{
							@:required:true,
							@:message:'请输入@(@item.Placeholder)',
@if(item.Trigger.Contains("["))
{
							@:trigger:@(@item.Trigger)
}
else
{
							@:trigger:'@(@item.Trigger)'
}

						@:},
}
@if(item.RegList!=null && item.RegList.Count > 0)
{
@foreach(var items in item.RegList)
{
						@:{
							@:pattern:@(@items.pattern),
							@:message:'@(@items.message)',
if(item.Trigger.Contains("["))
{
							@:trigger:@(@item.Trigger)
}
else
{
							@:trigger:'@(@item.Trigger)'
}
						@:},
}
}
					@:],
}
break;
}
}
                },
@foreach(var item in Model.OptionsList)
{
@switch(item.qtKey)
{
case "collapse":
				@:@(@item.Name):@(@item.Content),
break;
case "tab":
				@:@(@item.Name)Title:@(@item.Title),
				@:@(@item.Name):"@(@item.Content)",
break;
default:
@if(item.IsProps)
{
				@:@(@item.LowerName)Props:@(item.Props),
}
				@:@item.Content
break;
}
}
            };
        },
		watch: {
@if(Model.IsSummary)
{
			@:dataForm: {
				@:handler(val, oldVal) {
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "table":
@if(item.ShowSummary)
{
					@:this.@(@item.LowerName)();
}
break;
}
}
				@:},
				@:deep: true
			@:}				
}
        },
        created() {
@foreach(var item in @Model.OptionsList)
{
@if(!item.IsStatic && (item.IsChildren || item.IsIndex))
{
			@:this.get@(@item.LowerName)Options();
}
}
			@*this.selfInit();*@
        },
        onReady() {
            this.$refs.@(@Model.FormModel).setRules(this.rules);
        },
        methods: {
@foreach(var item in @Model.OptionsList)
{
@if(item.qtKey == "tab")
{
			@:@(@item.LowerName)Change(index)
			@:{
				@:this.@(@item.LowerName)Current = index;
			@:},
}
@if(!item.IsStatic && item.DictionaryType !=null)
{
			@:get@(@item.LowerName)Options(){
switch(@item.DataType)
{
case "dictionary":
				@:getDictionaryDataSelector('@(@item.DictionaryType)').then(res => {
					@:this.@(@item.LowerName)Options = res.data.list
	break;
case "dynamic":
				@:getDataInterfaceRes('@(@item.DictionaryType)').then(res => {
					@:let data = res.data.data
					@:this.@(@item.LowerName)Options = data
	break;
}
				@:});
			@:},
}
}
			selfGetInfo() {
				this.$store.commit('base/UPDATE_RELATION_DATA', {})
				this.loading = true
                request({
                    url: '/api/@(@Model.NameSpace)/@(@Model.ClassName)/' + this.dataForm.@(@Model.PrimaryKey),
                    method: 'get',
                }).then(res => {
					this.@(@Model.FormModel) = res.data
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "checkbox":
case "cascader":
case "uploadImg":
case "uploadFz":
case "address":
case "comSelect":
					@:if(!this.@(@Model.FormModel).@(@item.LowerName))this.@(@Model.FormModel).@(@item.LowerName)=[];
break;
}
}
					this.loading = false
                })
            },
			selfSubmit() {
                this.@(@Model.FormModel).flowState = this.eventType === 'submit' ? 0 : 1
                if (this.eventType === 'save') return this.selfHandleRequest()
                uni.showModal({
                    title: '提示',
                    content: '您确定要提交当前流程吗？',
                    success: res => {
                        if (res.confirm) {
                            this.selfHandleRequest()
                        }
                    }
                })
            },
			selfHandleRequest() {
                if (this.@(@Model.FormModel).@(@Model.PrimaryKey)) {
                    request({
                        url: '/api/@(@Model.NameSpace)/@(@Model.ClassName)/' + this.@(@Model.FormModel).@(@Model.PrimaryKey),
                        method: 'put',
                        data: this.@(@Model.FormModel),
                    }).then(res => {
                        uni.showToast({
                            title: res.msg,
                            complete: () => {
                                setTimeout(() => {
                                    uni.$emit('refresh')
                                    uni.navigateBack()
                                }, 1500)
                            }
                        })
                    })
                }
				else
				{
                    request({
                        url: '/api/@(@Model.NameSpace)/@(@Model.ClassName)',
                        method: 'post',
                        data: this.@(@Model.FormModel),
                    }).then(res => {
                        uni.showToast({
                            title: res.msg,
                            complete: () => {
                                setTimeout(() => {
                                    uni.$emit('refresh')
                                    uni.navigateBack()
                                }, 1500)
                            }
                        })
                    })
                }
            },
			selfInit(data){
				this.@(@Model.FormModel).flowId = data.flowId
@foreach(var item in Model.FormList)
{
@if(item.qtKey == "table")
{
				@:this.add@(@item.Name)()
}
}
			},
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "table":
			@:add@(@item.Name)() {
				@:let item = {
@foreach(var children in item.ChildrenList)
{
@switch(children.qtKey)
{
case "checkbox":
case "cascader":
case "uploadImg":
case "uploadFz":
case "address":
case "comSelect":
@if(children.DefaultValue == null || children.DefaultValue.ToString() == "")
{
					@:@(@children.LowerName):[],
}else{
					@:@(@children.LowerName):@(@children.DefaultValue),
}
break;
case "select":
case "userSelect":
case "treeSelect":
case "depSelect":
case "posSelect":
@if(children.Multiple)
{
if(@children.DefaultValue == null || @children.DefaultValue.ToString() == "")
{
					@:@(@children.LowerName):[],
}else{
					@:@(@children.LowerName):@(@children.DefaultValue),
}
}else{
if(@children.DefaultValue == null || @children.DefaultValue.ToString() == "")
{
					@:@(@children.LowerName):undefined,
}else{
					@:@(@children.LowerName):"@(@children.DefaultValue)",
}
}
break;
case "numInput":
case "date":
case "rate":
case "slider":
if(@children.DefaultValue == null || @children.DefaultValue.ToString() == "")
{
					@:@(@children.LowerName):undefined,
}else{
					@:@(@children.LowerName):@(@children.DefaultValue),
}
break;
case "switch":
					@:@(@children.LowerName):@(@children.DefaultValue ? "1" : "0"),
break;
default:
if(@children.DefaultValue == null || @children.DefaultValue.ToString() == "")
{
					@:@(@children.LowerName):undefined,
}else{
					@:@(@children.LowerName):"@(@children.DefaultValue)",
}
break;
}
}
				@:}
				@:this.@(@Model.FormModel).@(@item.OriginalName).push(item)
			@:},
			@:del@(@item.Name)(i) {
				@:this.@(@Model.FormModel).@(@item.OriginalName).splice(i, 1);
			@:},
@if(item.ShowSummary)
{
			@:@(@item.LowerName)(){
				 @:let table = this.@(@Model.FormModel).@(@item.OriginalName)
				 @:let summaryField = @(@item.SummaryField)
                 @:let summaryFieldName = {
@foreach(var items in item.ChildrenList)
{
@if(items.IsSummary)
{
					@:"@(@items.Name)":"@(@items.Placeholder)",
}
}
                 @:}
				 @:let data = {}
                 @:for (let i in summaryField) {
					@:let map = {}
					@:let val = 0
					@:for (let j = 0; j < table.length; j++) {
						@:let summary = table[j][summaryField[i]];
						@:if (summary) {
							@:let data = isNaN(summary) ? 0 : Number(summary)
							@:val += data
						@:}
					@:}
					@:map.id = summaryField[i];
					@:map.name = summaryFieldName[summaryField[i]];
					@:map.val = val;
					@:data[summaryField[i]] = map;
				@:}
				@:return data;
			@:},
}
break;
}
}
        }
    };
</script>

@{
    void GenCodeAppFormControls()
	{
@foreach(var item in Model.FormAllContols)
{
@switch(item.qtKey)
{
case "table":
			@:<view class="qt-table">
				@:<view class="qt-table-item" v-for="(item,i) in @(@Model.FormModel).@(@item.Name)" :key="i">
					@:<view class="qt-table-item-title u-flex u-row-between">
						@:<text class="qt-table-item-title-num">@(@item.Label)({{i+1}})</text>
						@:<template v-if="!setting.readonly && !judgeWrite('@(@item.Name)')">
							@:<view class="qt-table-item-title-action" v-if="@(@Model.FormModel).@(@item.Name).length > 1" @@click="del@(@item.ChildTableName)(i)">删除</view>
						@:</template>
                    @:</view>
@foreach(var children in item.Children)
{
@switch(children.qtKey)
{
case "uploadFz":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-file v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.IsDisabled)@(@children.Limit)@(@children.SizeUnit)@(@children.FileSize)@(@children.Accept)/>
					@:</u-form-item>
break;
case "comSelect":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
					    @:<qt-com-select type="organize" v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.Multiple)@(@children.IsDisabled)></qt-com-select>
					@:</u-form-item>
break;
case "address":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-city-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.Level)@(@children.IsDisabled)></qt-city-select>
					@:</u-form-item>
break;
case "billRule":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
                        @:<u-input v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" placeholder="系统自动生成" @(@children.IsDisabled)></u-input>
                    @:</u-form-item>
break;
case "userSelect":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-user-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.Multiple)@(@children.IsDisabled)></qt-user-select>
					@:</u-form-item>
break;
case "treeSelect":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-tree-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" :options="@(@children.LowerName)Options" :props="@(@children.LowerName)Props" @(@children.Placeholder)@(@children.Multiple)@(@children.IsDisabled)></qt-tree-select>
                    @:</u-form-item>
break;
case "posSelect":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-org-select type="position" v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.Multiple)@(@children.IsDisabled)></qt-org-select>
                    @:</u-form-item>
break;
case "depSelect":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-org-select type="department" v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.Multiple)@(@children.IsDisabled)></qt-org-select>
					@:</u-form-item>
break;
case "select":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder):options="@(@children.LowerName)Options" :props="@(@children.LowerName)Props" @(@children.Multiple)@(@children.IsDisabled)></qt-select>
					@:</u-form-item>
break;
case "cascader":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-cascader v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder):options="@(@children.LowerName)Options" :props="@(@children.LowerName)Props" @(@children.Multiple)@(@children.IsDisabled)></qt-cascader>
					@:</u-form-item>
break;
case "numInput":
                    @:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<u-number-box v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" :input-width="120" :input-height="60" :positive-integer="false" @(@children.Max)@(@children.Min)@(@children.Step)@(@children.IsDisabled)></u-number-box>
					@:</u-form-item>
break;
case "date":
case "time":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-date-time @(@children.Type)v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.IsDisabled)></qt-date-time>
					@:</u-form-item>
break;
case "switch":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<view class="u-flex u-form-item-switch">
							@:<qt-switch v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)"  @(@children.IsDisabled)></qt-switch>
						@:</view>
					@:</u-form-item>
break;
case "comInput":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<u-input v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.Placeholder)@(@children.IsDisabled)></u-input>
					@:</u-form-item>
break;
@*关联表单*@
case "relationForm":
					@:<u-form-item  label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-relation-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.IsDisabled)@(@children.RelationField)@(@children.ColumnOptions)@(@children.ModelId)@(@children.Placeholder)@(@children.HasPage)@(@children.PageSize):vModel="'@(@children.Name)'+i" type="relation"></qt-relation-select>
					@:</u-form-item>
break;
@*关联表单属性*@
case "relationFormAttr":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.RelationField)">
						@:<qt-relation-attr showField="@(@children.ShowField)" :relationField="'@(@children.RelationField)'+i" type='relationFormAttr'></qt-relation-attr>
					@:</u-form-item>
break;
@*弹窗选择*@
case "popupSelect":
					@:<u-form-item label="@(@children.Label)"prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<qt-popup-select v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" @(@children.IsDisabled)@(@children.PropsValue)@(@children.RelationField)@(@children.ColumnOptions)@(@children.InterfaceId)@(@children.Placeholder)@(@children.HasPage)@(@children.PageSize)@(@children.PopupType)@(@children.PopupTitle)@(@children.PopupWidth):vModel="'@(@children.Name)'+i" type="popup"></qt-popup-select>
					@:</u-form-item>
break;
@*弹窗选择属性*@
case "popupAttr":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.RelationField)">
						@:<qt-relation-attr showField="@(@children.ShowField)" :relationField="'@(@children.RelationField)'+i" type='popupAttr'></qt-relation-attr>
					@:</u-form-item>
break;
case "textarea":
					@:<u-form-item label="@(@children.Label)" prop="@(@children.LowerName)" v-if="judgeShow('@(@item.Name)-@(@children.LowerName)')" :required="requiredList['@(@item.Name + "-" + @children.LowerName)']">
						@:<u-input v-model="@(@Model.FormModel).@(@item.Name)[i].@(@children.LowerName)" type="textarea" @(@children.MaxLength)@(@children.Placeholder)@(@children.IsDisabled)/>
					@:</u-form-item>
break;
}
}
                @:</view>
				@:<view class="qt-table-addBtn" @@click="add@(@item.ChildTableName)" v-if="!setting.readonly && !judgeWrite('@(@item.Name)')">
					@:<u-icon name="plus" color="#2979ff"></u-icon>添加
				@:</view>
@if(item.ShowSummary)
{
				@:<view class="qt-table-item">
					@:<view class="qt-table-item-title u-flex u-row-between">
						@:<text class="qt-table-item-title-num">@(@item.Label)合计</text>
					@:</view>
					@:<u-form-item v-for="(item,i) in @(@item.LowerChildTableName)()" :key="i" :label="item.name">
						@:<p>{{item.val}}</p>
					@:</u-form-item>
				@:</view>
}
			@:</view>
break;
case "groupTitle":
			@:<qt-group content="@(@item.Content)" content-position="@(@item.Contentposition)"></qt-group>
break;
case "divider":
			@:<u-divider half-width="200" height="80">@(@item.Default)</u-divider>
break;
case "card":
case "row":
			@:<view class="qt-card">
				@:<view class="qt-card-cap u-line-1">@(@item.Content)</view>
@{GenCodeAppFormChildrenControls(item.Children);}
			@:</view>
break;
case "comInput":
			@:<u-form-item label="@(@item.Label)" prop="@(@item.LowerName)" @(@item.IsRequired)>
				@:<u-input v-model="@(@Model.FormModel).@(@item.LowerName)" @(@item.MaxLength)@(@item.Placeholder)@(@item.IsDisabled)></u-input>
			@:</u-form-item>
break;
case "textarea":
			@:<u-form-item label="@(@item.Label)" prop="@(@item.LowerName)" @(@item.IsRequired)>
				@:<u-input v-model="@(@Model.FormModel).@(@item.LowerName)" type="textarea" @(@item.MaxLength)@(@item.Placeholder)@(@item.IsDisabled)/>
            @:</u-form-item>
break;
case "numInput":
			@:<u-form-item label="@(@item.Label)" prop="@(@item.LowerName)" @(@item.IsRequired)>
