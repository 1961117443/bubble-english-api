{
  "flowEngine": {
    "EnCode": "@(@Model.EnCode)",
    "FullName": "@(@Model.FullName)",
    "Type": 1,
    "Category": "@(@Model.Category)",
    "Form": null,
    "VisibleType": 0,
    "Icon": "icon-qt icon-qt-node",
    "IconBackground": "#008cff",
    "Version": null,
    "FlowTemplateJson": @(@Model.FlowTemplateJson),
    "Description": null,
    "SortCode": 0,
    "FormTemplateJson": "[
@{ var formListNum = 0;}
@foreach(var item in Model.FormList)
{
@switch(item.qtKey)
{
case "table":
            @:@(formListNum > 0 ? "," : ""){\"filedName\":\"@(@item.Placeholder)\",\"filedId\":\"@(@item.OriginalName)\",\"required\":@(@item.Required ==true ? "true":" false")}
formListNum++;
@foreach(var children in item.ChildrenList)
{
            @:@(formListNum > 0 ? "," : ""){\"filedName\":\"@(@item.Placeholder+"-"+@children.Placeholder)\",\"filedId\":\"@(@item.OriginalName+"-"+@children.LowerName)\",\"required\":@(@children.Required ==true ? "true":" false")}
}
break;
default:
            @:@(formListNum > 0 ? "," : ""){\"filedName\":\"@(@item.Placeholder)\",\"filedId\":\"@(@item.LowerName)\",\"required\":@(@item.Required ==true ? "true":" false")}
formListNum++;
break;
}
}
    ]",
    "FormType": 1,
    "Tables": @(@Model.Tables),
    "DbLinkId": "@(@Model.DbLinkId)",
    "AppFormUrl": null,
    "FormUrl": "extend/@(@Model.MianTable)/Form.vue",
    "CreatorTime": @Model.CreatorTime,
    "CreatorUserId": "@(@Model.CreatorUserId)",
    "EnabledMark": 1,
    "LastModifyTime": null,
    "LastModifyUserId": null,
    "DeleteMark": null,
    "DeleteTime": null,
    "DeleteUserId": null,
    "Version":1,
    "Id": "@(@Model.FlowId)"
  },
  "visibleList": [
  ]
}