using QT.Common.Filter;

namespace QT.@(@Model.NameSpace).Entitys.Dto.@Model.ClassName;

/// <summary>
/// @(@Model.BusName)列表查询输入
/// </summary>
public class @(@Model.ClassName)ListQueryInput : PageInputBase
{
    /// <summary>
    /// 选择导出数据key.
    /// </summary>
    public string selectKey { get; set; }

    /// <summary>
    /// 导出类型.
    /// </summary>
    public int dataType { get; set; }

@foreach (var column in Model.TableField)
{
@if (column.QueryWhether)
{
@switch(column.IsAuxiliary)
{
case true:
    @:/// <summary>
    @:/// @column.ColumnComment.
    @:/// </summary>
    @:public string qt_@(@column.TableName)_qt_@column.LowerColumnName { get; set; }
@:
break;
default:
    @:/// <summary>
    @:/// @column.ColumnComment.
    @:/// </summary>
    @:public string @column.LowerColumnName { get; set; }
@:
break;
}
}
}
}